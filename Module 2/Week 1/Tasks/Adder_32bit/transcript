# adder_32.wlf opened as dataset "adder_32"
add wave -position end  adder_32:/adder32_tb/a
add wave -position end  adder_32:/adder32_tb/b
add wave -position end  adder_32:/adder32_tb/cin
add wave -position end  adder_32:/adder32_tb/sum
add wave -position end  adder_32:/adder32_tb/cout
